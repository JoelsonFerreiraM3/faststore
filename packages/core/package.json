    "@sentry/integrations": "^7.106.0",
    "@sentry/nextjs": "^7.93.0",
    "@types/react": "^18.2.42",
    "@types/react-image-gallery": "^1.2.4",
    "@vtex/client-cms": "^0.2.12",
    "@vtex/prettier-config": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "chalk": "^5.2.0",
